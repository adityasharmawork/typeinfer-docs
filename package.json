{
  "name": "typeinfer",
  "version": "1.0.3",
  "description": "Automatically generate TypeScript interfaces or JSON schemas from various data sources",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "typeinfer": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "prepare": "npm run build"
  },
  "keywords": [
    "typescript",
    "json-schema",
    "type-inference",
    "code-generation",
    "cli"
  ],
  "author": "Aditya Sharma",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^5.4.1",
    "chalk-animation": "^2.0.3",
    "commander": "^11.1.0",
    "csv-parser": "^3.0.0",
    "gradient-string": "^3.0.0",
    "inquirer": "^12.6.0",
    "mongodb": "^6.0.0",
    "nanospinner": "^1.2.2",
    "prettier": "^3.1.0"
  },
  "devDependencies": {
    "@types/chalk-animation": "^1.6.3",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
